.Home {
  width: 100%;
}
.Home .navbar-title {
  width: 100%;
  height: 100px;
  background-color: #1296db;
}
.Home .navbar-title .occupy {
  width: 100%;
  height: 55%;
}
.Home .navbar-title .navigationBar {
  width: 100%;
  height: 40%;
  display: flex;
  align-items: center;
  font-size: 12px;
}
.Home .navbar-title .navigationBar .navigationBar-positioning {
  width: 15%;
  height: 100%;
  display: flex;
  margin: 0 2%;
}
.Home .navbar-title .navigationBar .navigationBar-positioning .images {
  flex: 1;
  height: 100%;
  text-align: center;
  line-height: 45px;
}
.Home .navbar-title .navigationBar .navigationBar-positioning .images image {
  width: 90%;
  height: 80%;
  color: white;
}
.Home .navbar-title .navigationBar .navigationBar-positioning .text {
  width: 50%;
  height: 100%;
  text-align: left;
  line-height: 30px;
  color: white;
}
.Home .navbar-title .navigationBar .navigationBar-search {
  flex: 1;
  opacity: 0.3;
  justify-items: center;
  align-items: center;
}
.Home .navbar-title .navigationBar .navigationBar-ioc {
  display: flex;
}
.Home .navbar-title .navigationBar .navigationBar-ioc .ioc_ {
  width: 30px;
  height: 30px;
  border: 1px solid black;
  margin: 0 1px;
}