.enterprise {
  width: 100%;
  height: 1vh;
  background-color: #f7f7f7;
}
.enterprise .list {
  width: 100%;
  height: 100px;
  margin-top: 20px;
  background-color: #fff;
}
.enterprise .list .name {
  width: 100%;
  height: 50%;
  border-bottom: 1px solid #f8f8f8;
  display: flex;
}
.enterprise .list .name .text {
  width: 85%;
  height: 100%;
  font-size: 15px;
  display: flex;
  align-items: center;
  padding-left: 10px;
  font-weight: "粗体";
}
.enterprise .list .name .ioc {
  flex: 1;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.enterprise .list .name .ioc image {
  width: 6px;
  height: 12px;
}
.enterprise .list .btn {
  width: 100%;
  height: 50%;
  display: flex;
}
.enterprise .list .btn .btn-content {
  width: 100%;
  padding-top: 10px;
}
.enterprise .list .btn .btn-content button {
  margin-right: 5px;
  float: right;
  color: #6a87c5;
  background: #fff;
  border-color: #6a87c5;
}
.enterprise .btn .add {
  width: 90%;
  background: #4a79e0;
  border-radius: 30px;
}
.enterprise .bg-ioc {
  margin-top: 30px;
  width: 100%;
  height: 50rpx;
  margin-bottom: 20px;
  display: flex;
  justify-content: center;
  align-items: center;
}
.enterprise .bg-ioc image {
  width: 65%;
  height: 65%;
}
.enterprise .serve {
  width: 100%;
  margin-top: -10px;
}
.enterprise .serve .serveList {
  width: 100%;
  height: 100px;
  background: url("data:image/png;base64,") no-repeat;
  border-radius: 5px;
  background-size: 100%;
  margin: 0 auto;
  display: flex;
}
.enterprise .serve .serveList .text {
  width: 70%;
  height: 100%;
}
.enterprise .serve .serveList .text .title {
  width: 100%;
  height: 50%;
  display: flex;
  margin-top: 10rpx;
  margin-left: 10rpx;
  align-items: center;
}
.enterprise .serve .serveList .text .title image {
  width: 25px;
  height: 25px;
  margin: 0 5px 0 20px;
}
.enterprise .serve .serveList .text .miniTitle {
  width: 100%;
  font-size: 12px;
  color: #c6c6c6;
  margin-left: 15px;
}
.enterprise .serve .serveList .ioc {
  flex: 1;
  height: 100%;
  display: flex;
  align-items: center;
}